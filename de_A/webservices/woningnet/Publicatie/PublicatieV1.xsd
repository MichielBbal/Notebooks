<?xml version="1.0" encoding="UTF-8"?>
<xs:schema targetNamespace="http://www.woningnet.nl/Contracten/Aanbod/Publicatie" elementFormDefault="qualified" attributeFormDefault="unqualified" xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns:obj="http://www.woningnet.nl/Contracten/Aanbod/Object" xmlns:pub="http://www.woningnet.nl/Contracten/Aanbod/Publicatie" xmlns:com="http://www.woningnet.nl/Contracten/Common">
  <xs:import namespace="http://www.woningnet.nl/Contracten/Common" schemaLocation="CommonV1.xsd"/>
  <xs:import namespace="http://www.woningnet.nl/Contracten/Aanbod/Object" schemaLocation="ObjectV1.xsd"/>
  <xs:element name="PublicatieXml" type="pub:PublicatieXml" nillable="true"/>
  <xs:element name="PublicatieSynchronisatie" type="pub:PublicatieSynchronisatie" nillable="true"/>
  <xs:element name="PublicatieKey" type="pub:PublicatieKey" nillable="true"/>
  <xs:element name="Publicatie" type="pub:Publicatie" nillable="true"/>
  <xs:element name="Site" type="pub:Site"/>
  <xs:element name="ZoekFlow" type="pub:ZoekFlow"/>
  <xs:element name="ActueelAanbodKey" type="pub:ActueelAanbodKey"/>
  <xs:element name="PublicatieKeys" type="pub:PublicatieKeys" nillable="true"/>
