      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="PublicatieKeys">
    <xs:sequence>
      <xs:element name="PublicatieKey" type="pub:PublicatieKey" minOccurs="0" maxOccurs="unbounded"/>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="Oplevering">
    <xs:sequence>
      <xs:element name="Datum" type="xs:dateTime" nillable="true" minOccurs="0"/>
      <xs:element name="Code" type="xs:string"/>
      <xs:element name="Omschrijving" type="xs:string" minOccurs="0"/>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="TaalPublicatie">
    <xs:sequence>
      <xs:element name="TaalId" type="xs:integer"/>
