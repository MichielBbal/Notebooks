      <xs:element name="StartDatum" type="xs:dateTime"/>
      <xs:element name="EindDatum" type="xs:dateTime" nillable="true" minOccurs="0"/>
      <xs:element name="XML">
        <xs:complexType>
          <xs:sequence>
            <xs:any processContents="lax" minOccurs="0"/>
          </xs:sequence>
        </xs:complexType>
      </xs:element>
      <xs:element ref="obj:ObjectKey" minOccurs="0"/>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="PublicatieSynchronisatie">
