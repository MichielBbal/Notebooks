      <xs:element name="PublicatieKey" type="pub:PublicatieKey"/>
      <xs:element ref="obj:ObjectKey" minOccurs="0"/>
      <xs:element name="TaalPublicaties" nillable="false">
        <xs:complexType>
          <xs:sequence>
            <xs:element name="TaalPublicatie" type="pub:TaalPublicatie" maxOccurs="unbounded"/>
          </xs:sequence>
        </xs:complexType>
      </xs:element>
      <xs:element name="StartDatum" type="xs:dateTime"/>
      <xs:element name="EindDatum" type="xs:dateTime" nillable="true" minOccurs="0"/>
      <xs:element name="GemReactiesRegio" type="xs:integer" nillable="true" minOccurs="0"/>
      <xs:element name="GemWeigeringenRegio" type="xs:integer" nillable="true" minOccurs="0"/>
      <xs:element name="GemReactiesGelijkObject" type="xs:integer" nillable="true" minOccurs="0"/>
      <xs:element name="GemWeigeringenGelijkObject" type="xs:integer" nillable="true" minOccurs="0"/>
      <xs:element name="AantalPublicaties" type="xs:integer" nillable="true" minOccurs="0"/>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="Site">
    <xs:sequence>
      <xs:element name="Site" type="xs:string" minOccurs="0" maxOccurs="unbounded"/>
