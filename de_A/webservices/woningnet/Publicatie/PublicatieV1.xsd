        <xs:complexType>
          <xs:sequence>
            <xs:any processContents="lax" minOccurs="0"/>
          </xs:sequence>
        </xs:complexType>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:simpleType name="Integer">
    <xs:restriction base="xs:integer"/>
  </xs:simpleType>
</xs:schema>