<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema">
	<xs:simpleType name="datetime">
		<xs:annotation>
			<xs:documentation>ISO Date time format with (optional) time zone designator: YYYY-MM-DDTHH:MM:SSÂ±hh:mm.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:dateTime"/>
	</xs:simpleType>
	<xs:simpleType name="id">
		<xs:annotation>
			<xs:documentation>unique identifier of the entity</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:maxLength value="40"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:element name="planning">
		<xs:complexType>
			<xs:sequence>
				<xs:element name="task" maxOccurs="unbounded">
					<xs:complexType>
						<xs:sequence>
							<xs:element name="id" type="id"/>
							<xs:element name="planned" maxOccurs="unbounded">
								<xs:complexType>
									<xs:sequence minOccurs="0">
										<xs:element name="resourceId" type="id"/>
										<xs:element name="from" type="datetime"/>
										<xs:element name="till" type="datetime"/>
									</xs:sequence>
								</xs:complexType>
							</xs:element>
							<xs:element name="extraInfo" minOccurs="0">
								<xs:complexType>
									<xs:sequence>
										<xs:element name="info1" minOccurs="0"/>
										<xs:element name="info2" minOccurs="0"/>
										<xs:element name="info3" minOccurs="0"/>
										<xs:element name="info4" minOccurs="0"/>
										<xs:element name="info5" minOccurs="0"/>
									</xs:sequence>
								</xs:complexType>
							</xs:element>
						</xs:sequence>
					</xs:complexType>
				</xs:element>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
</xs:schema>
