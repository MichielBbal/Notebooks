    <xs:complexType>
      <xs:sequence>
        <xs:element ref="egr:VestigingKey"/>
        <xs:element ref="adr:Adres"/>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <xs:complexType name="EigenarenKey">
    <xs:sequence>
      <xs:element name="Regiocode" type="xs:string"/>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="EigenaarKey">
    <xs:sequence>
      <xs:element name="EigenaarId" type="xs:integer"/>
      <xs:element name="Regiocode" type="xs:string"/>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="Eigenaren">
    <xs:sequence>
      <xs:element name="Eigenaar" type="egr:Eigenaar" minOccurs="0" maxOccurs="unbounded" nillable="true"/>
