<?xml version="1.0" encoding="UTF-8"?>
<xs:schema targetNamespace="http://www.woningnet.nl/Contracten/Eigenaar" elementFormDefault="qualified" attributeFormDefault="unqualified" xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns:egr="http://www.woningnet.nl/Contracten/Eigenaar" xmlns:adr="http://www.woningnet.nl/Contracten/Common/Adressen" xmlns:rda="http://www.woningnet.nl/Contracten/Algemeen/Referentiedata">
  <xs:import namespace="http://www.woningnet.nl/Contracten/Common/Adressen" schemaLocation="AdressenV1.xsd"/>
  <xs:import namespace="http://www.woningnet.nl/Contracten/Algemeen/Referentiedata" schemaLocation="ReferentiedataV1.xsd"/>
  <xs:element name="EigenaarKey" type="egr:EigenaarKey"/>
  <xs:element name="EigenarenKey" type="egr:EigenarenKey"/>
  <xs:element name="Eigenaren" type="egr:Eigenaren" nillable="true"/>
  <xs:element name="Eigenaar" type="egr:Eigenaar" nillable="true"/>
  <xs:element name="EigenaarGroepKey" type="egr:EigenaarGroepKey"/>
  <xs:element name="EigenaarGroep" type="egr:EigenaarGroep" nillable="true"/>
  <xs:element name="Vestiging" type="egr:Vestiging" nillable="true"/>
  <xs:element name="Vestigingen" type="egr:Vestigingen" nillable="true"/>
