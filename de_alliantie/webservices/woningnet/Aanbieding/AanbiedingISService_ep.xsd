  <xsd:import namespace="http://www.woningnet.nl/Contracten/Common/Meldingen" schemaLocation="MeldingenV1.xsd"/>
  <xsd:import namespace="http://www.woningnet.nl/Contracten/Wozo/Aanbieding" schemaLocation="AanbiedingV1.xsd"/>
  <xsd:element name="GetKandidatenRequest">
